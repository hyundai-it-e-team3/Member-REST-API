<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mycompany.memberAPI.dao.UsePointDao">
	<insert id="insertPoint" parameterType="usePoint">
		<selectKey keyProperty="usePointSeq" resultType="int" order="BEFORE">
			SELECT seq_use_point.NEXTVAL FROM DUAL
		</selectKey>
		INSERT INTO use_point
		VALUES (#{usePointSeq}, #{point}, #{useType}, sysdate)
	</insert>
</mapper>